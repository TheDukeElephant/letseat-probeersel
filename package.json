{
  "name": "letseat-probeersel",
  "version": "1.0.0",
  "description": "This repository contains the codebase for the \"Let's Eat\" project, structured as a monorepo using [TurboRepo](https://turbo.build/). It includes multiple applications and shared packages.",
  "main": "index.js",
  "scripts": {
    "dev": "turbo run dev",
    "expo:mobile": "cd apps/mobile && npx expo start",
    "build": "turbo run build",
    "lint": "turbo run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheDukeElephant/letseat-probeersel.git"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*"
  ],
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheDukeElephant/letseat-probeersel/issues"
  },
  "homepage": "https://github.com/TheDukeElephant/letseat-probeersel#readme",
  "devDependencies": {
    "babel-plugin-module-resolver": "^5.0.2",
    "turbo": "^2.5.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-regular-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3"
  }
}
